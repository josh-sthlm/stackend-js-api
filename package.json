{
  "name": "@stackend/api",
  "version": "1.0.71",
  "description": "JS bindings to api.stackend.com",
  "keywords": [
    "comments",
    "cms",
    "code bin",
    "article comments",
    "reviews",
    "community feed",
    "login",
    "registration",
    "user profiles",
    "oAuth2",
    "pages",
    "sites",
    "moderation",
    "feed",
    "admin",
    "user generated",
    "rating",
    "threaded comments",
    "headless cms",
    "decoupled cms",
    "react cms"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/josh-sthlm/stackend-js-api"
  },
  "module": "api/index.js",
  "bugs": {
    "email": "info@stackend.com"
  },
  "author": "Stackend",
  "homepage": "https://stackend.com",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "lint": "eslint --ext ts src tests",
    "clean": "rimraf types lib {abuse,api,blog,category,cms,comments,counter,event,forum,group,like,login,media,poll,qna,rating,request,search,shop,stackend,throbber,user,util,vote}",
    "format:check": "prettier --list-different \"{src,tests}/**/*.{js,ts}\" \"**/*.md\"",
    "format": "prettier --config .prettierrc.json \"{src,tests}/**/*.ts\" \"**/*.md\" --write",
    "prepack": "npm run clean && npm run build",
    "prepublishOnly": "npm run lint && npm run format:check && npm run test",
    "preversion": "npm run lint"
  },
  "types": "./",
  "peerDependencies": {
    "redux": "4.x",
    "redux-thunk": "2.x",
    "winston": "3.x"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "es6-promise": "^3.3.1",
    "immutability-helper": "^2.9.1",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.20",
    "@shopify/address": "^2.9.2",
    "@shopify/address-consts": "^2.1.1"
  },
  "devDependencies": {
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "winston": "^3.3.3",
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/plugin-external-helpers": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-flow": "^7.12.1",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.1",
    "@types/config": "^0.0.36",
    "@types/jest": "^26.0.16",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.19.8",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^2.5.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2",
    "typings-tester": "^0.3.2",
    "core-js": "^2.6.11"
  },
  "jest": {
    "testRegex": "(/tests/.*\\.spec\\.[tj]s)$",
    "coverageProvider": "v8"
  },
  "publishConfig": {
    "access": "public"
  }
}
