{
  "name": "@stackend/api",
  "version": "1.4.63",
  "description": "JS bindings to api.stackend.com",
  "keywords": [
    "comments",
    "cms",
    "code bin",
    "article comments",
    "reviews",
    "community feed",
    "login",
    "registration",
    "user profiles",
    "oAuth2",
    "pages",
    "sites",
    "moderation",
    "feed",
    "admin",
    "user generated",
    "rating",
    "threaded comments",
    "headless cms",
    "decoupled cms",
    "react cms"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/josh-sthlm/stackend-js-api"
  },
  "module": "api/index.js",
  "bugs": {
    "email": "info@stackend.com"
  },
  "author": "Stackend",
  "homepage": "https://stackend.com",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest --config=jest.config.js",
    "lint": "eslint --ext ts src tests",
    "clean": "rimraf types lib {abuse,api,blog,category,cms,comments,counter,event,forum,group,like,live,login,media,moderation,poll,qna,rating,request,search,shop,stackend,throbber,user,util,vote}",
    "format:check": "prettier --list-different \"{src,tests}/**/*.ts\" \"**/*.md\"",
    "format": "prettier --config .prettierrc.json \"{src,tests}/**/*.ts\" \"**/*.md\" --write",
    "prepack": "npm run clean && npm run build",
    "prepublishOnly": "npm run lint && npm run format:check && npm run test",
    "preversion": "npm run lint"
  },
  "types": "./",
  "peerDependencies": {
    "redux": "4.x",
    "redux-thunk": "2.x",
    "shopify-buy": "2.x"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.4",
    "es6-promise": "^4.2.8",
    "immutability-helper": "^3.1.1",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.21",
    "shopify-buy": "2.x",
    "sockjs-client": "^1.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/node": "^7.19.1",
    "@babel/plugin-external-helpers": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.19.4",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.11.7",
    "@types/shopify-buy": "^2.10.10",
    "@types/sockjs-client": "^1.5.1",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.22.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^27.3.1",
    "jest-environment-jsdom": "27.3.1",
    "jest-localstorage-mock": "^2.4.21",
    "msw": "1.0.0",
    "prettier": "^2.6.2",
    "redux": "^4.1.2",
    "redux-thunk": "^2.3.0",
    "rimraf": "^3.0.2",
    "shopify-buy": "^2.14.0",
    "ts-jest": "^27.1.5",
    "typescript": "^4.6.4"
  }
