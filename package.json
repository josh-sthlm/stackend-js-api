{
  "name": "@stackend/api",
  "version": "1.0.57",
  "description": "JS bindings to api.stackend.com",
  "keywords": [
    "comments",
    "cms",
    "code bin",
    "article comments",
    "reviews",
    "community feed",
    "login",
    "registration",
    "user profiles",
    "oAuth2",
    "pages",
    "sites",
    "moderation",
    "feed",
    "admin",
    "user generated",
    "rating",
    "threaded comments",
    "headless cms",
    "decoupled cms",
    "react cms"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/josh-sthlm/stackend-js-api"
  },
  "module": "api/index.js",
  "bugs": {
    "email": "info@stackend.com"
  },
  "author": "Stackend",
  "homepage": "https://stackend.com",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "lint": "eslint --ext ts src tests",
    "clean": "rimraf types lib {abuse,api,blog,category,cms,comments,counter,event,forum,group,like,login,media,poll,qna,rating,request,search,shop,stackend,throbber,user,util,vote}",
    "format:check": "prettier --list-different \"{src,tests}/**/*.{js,ts}\" \"**/*.md\"",
    "format": "prettier --config .prettierrc.json \"{src,tests}/**/*.ts\" \"**/*.md\" --write",
    "prepack": "npm run clean && npm run build",
    "prepublishOnly": "npm run lint && npm run format:check && npm run test",
    "preversion": "npm run lint"
  },
  "types": "./",
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "es6-promise": "^3.3.1",
    "immutability-helper": "^2.9.1",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.20",
    "winston": "^3.3.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "@shopify/address": "^2.9.1",
    "@shopify/address-consts": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-external-helpers": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-flow": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.11.5",
    "@types/config": "^0.0.36",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.161",
    "@types/node": "^10.17.29",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^2.5.1",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.2",
    "typings-tester": "^0.3.2",
    "core-js": "^2.6.11"
  },
  "jest": {
    "testRegex": "(/tests/.*\\.spec\\.[tj]s)$",
    "coverageProvider": "v8"
  },
  "publishConfig": {
    "access": "public"
  }
}
