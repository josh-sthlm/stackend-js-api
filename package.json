{
  "name": "@stackend/api",
  "version": "1.2.17",
  "description": "JS bindings to api.stackend.com",
  "keywords": [
    "comments",
    "cms",
    "code bin",
    "article comments",
    "reviews",
    "community feed",
    "login",
    "registration",
    "user profiles",
    "oAuth2",
    "pages",
    "sites",
    "moderation",
    "feed",
    "admin",
    "user generated",
    "rating",
    "threaded comments",
    "headless cms",
    "decoupled cms",
    "react cms"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/josh-sthlm/stackend-js-api"
  },
  "module": "api/index.js",
  "bugs": {
    "email": "info@stackend.com"
  },
  "author": "Stackend",
  "homepage": "https://stackend.com",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "lint": "eslint --ext ts src tests",
    "clean": "rimraf types lib {abuse,api,blog,category,cms,comments,counter,event,forum,group,like,live,login,media,moderation,poll,qna,rating,request,search,shop,stackend,throbber,user,util,vote}",
    "format:check": "prettier --list-different \"{src,tests}/**/*.ts\" \"**/*.md\"",
    "format": "prettier --config .prettierrc.json \"{src,tests}/**/*.ts\" \"**/*.md\" --write",
    "prepack": "npm run clean && npm run build",
    "prepublishOnly": "npm run lint && npm run format:check && npm run test",
    "preversion": "npm run lint"
  },
  "types": "./",
  "peerDependencies": {
    "redux": "4.x",
    "redux-thunk": "2.x"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "es6-promise": "^4.2.8",
    "immutability-helper": "^3.1.1",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.21",
    "sockjs-client": "^1.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.14.9",
    "@babel/plugin-external-helpers": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-flow": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^12.20.19",
    "@types/sockjs-client": "^1.5.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-eslint": "^10.1.0",
    "core-js": "^2.6.12",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-flowtype": "^5.9.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "jest-localstorage-mock": "^2.4.16",
    "prettier": "^2.3.2",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  },
  "jest": {
    "testRegex": "(/tests/.*\\.spec\\.[tj]s)$",
    "coverageProvider": "v8"
  }
}
