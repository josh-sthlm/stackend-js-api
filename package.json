  "scripts": {
    "build": "tsc",
    "pretest": "npm run build",
    "test": "jest",
    "lint": "eslint --ext js,ts src tests",
    "clean": "rimraf build coverage types",
    "format:check": "prettier --list-different \"{src,tests}/**/*.{js,ts}\" \"**/*.md\"",
    "format": "prettier --config .prettierrc.json 'src/**/*.ts' --write",
    "prepublishOnly": "npm run clean && npm run check-types && npm run format:check && npm run lint && npm test"
  },
  "jest": {
    "testRegex": "(/tests/.*\\.spec\\.[tj]s)$",
    "coverageProvider": "v8"
  },
  "types": "./types/"
